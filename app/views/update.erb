<h3>Update <%=meet_up.title%></h3>
<form action="/<%=meet_up.id%>" method="post">
  <input type="hidden" name="_method" value="PATCH">

  <label for="title"> Title </label>
  <input type="text" value="<%=meet_up.title%>" class='button'name="title">

  <label for="description">Description </label>
  <input type="text" value="<%=meet_up.description%>" class='button' name="description">

  <label for="location"> Location </label>
  <input type="text" value="<%=meet_up.location%>" class='button' name="location">

  <input type="submit" class='button' value="Update Meet Up">
</form>
